{
  "name": "beautiful-ui",
  "version": "0.23.0",
  "description": "A charming set of beautiful ui components",
  "main": "dist/index.js",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "NODE_ENV=production npm test && npm run clean-build && npm run build-js && npm run build-style",
    "build-js": "babel src --out-dir dist --source-maps --config-file ./babel.build.js",
    "build-style": "sass src:dist -s compressed && webpack --config webpack.style.build.config.js && rm dist/delete.this.*",
    "clean-build": "del-cli dist",
    "start": "styleguidist server --config ./styleguidist.config.js",
    "lint-js": "eslint --ext .jsx,.js src/",
    "lint-scss": "stylelint '**/*.scss'",
    "lint": "npm run lint-js && npm run lint-scss",
    "test": "nyc mocha",
    "tdd": "npm test -- --watch --watch-extensions jsx"
  },
  "author": "Antonio Russo @ Beautiful Interactions",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/beautifulinteractions/beautiful-ui.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@testing-library/react": "^8.0.8",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "chai": "^4.2.0",
    "css-loader": "^3.2.0",
    "del-cli": "^2.0.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-chai-expect": "^2.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.1.0",
    "husky": "^3.0.2",
    "ignore-styles": "^5.0.1",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^6.2.0",
    "node-sass": "^4.12.0",
    "nyc": "^14.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss": "^7.0.17",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-syntax": "^0.36.2",
    "react-styleguidist": "^9.1.13",
    "sass": "^1.22.9",
    "sass-loader": "^7.1.0",
    "sinon": "^7.4.1",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-config-sass-guidelines": "^6.0.0",
    "stylelint-config-standard": "^18.3.0",
    "tailwindcss": "^1.1.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.21",
    "@fortawesome/free-brands-svg-icons": "^5.10.1",
    "@fortawesome/free-solid-svg-icons": "^5.10.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "lodash": "^4.17.15"
  },
  "nyc": {
    "check-coverage": true,
    "include": [
      "src"
    ],
    "exclude": [
      "src/**/*.spec.js",
      "src/**/index.js"
    ],
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
  }
}
