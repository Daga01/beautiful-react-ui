{
  "name": "beautiful-ui",
  "version": "0.46.0",
  "description": "A collection of lightweight and easy-to-customise controlled React components",
  "main": "dist/index.js",
  "files": [
    "dist/**",
    "@types/**",
    "index.d.ts"
  ],
  "types": "index.d.ts",
  "scripts": {
    "build": "NODE_ENV=production gulp -f ./scripts/gulpfile.js",
    "build-doc": "npx styleguidist build --config docs/setup/styleguidist.config.js",
    "start": "styleguidist server --config docs/setup/styleguidist.config.js",
    "lint-js": "eslint --ext .jsx,.js src/",
    "lint-scss": "stylelint '**/*.scss'",
    "lint": "npm run lint-js && npm run lint-scss",
    "test": "nyc mocha",
    "tdd": "npm test -- --watch --watch-extensions js"
  },
  "author": "Antonio Russo @ Beautiful Interactions",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/beautifulinteractions/beautiful-ui.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/register": "^7.7.0",
    "@testing-library/react": "^9.3.2",
    "autoprefixer": "^9.7.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "chai": "^4.2.0",
    "css-loader": "^3.2.0",
    "del": "^5.1.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-chai-expect": "^2.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-cli": "^2.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "husky": "^3.0.9",
    "ignore-styles": "^5.0.1",
    "jsdom": "^15.2.1",
    "jsdom-global": "^3.0.2",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^6.2.2",
    "node-sass": "^4.13.0",
    "nyc": "^14.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss": "^7.0.21",
    "postcss-fixes": "^2.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-will-change": "^3.0.0",
    "postcss-will-change-transition": "^1.2.0",
    "react-styleguidist": "^10.2.0",
    "sass": "^1.23.3",
    "sass-loader": "^8.0.0",
    "sinon": "^7.5.0",
    "stylelint": "^11.1.1",
    "stylelint-config-recommended-scss": "^4.0.0",
    "stylelint-config-sass-guidelines": "^6.1.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.13.0",
    "tailwindcss": "^1.1.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "nyc": {
    "check-coverage": true,
    "include": [
      "src"
    ],
    "exclude": [
      "src/**/*.spec.js",
      "src/**/index.js"
    ],
    "branches": 75,
    "lines": 80,
    "functions": 80,
    "statements": 80
  }
}
